<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@android:color/white"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <com.azusasoft.facehubcloudsdk.views.viewUtils.FacehubActionbar
            android:id="@+id/actionbar_facehub"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <com.azusasoft.facehubcloudsdk.views.advrecyclerview.RecyclerViewEx
            android:id="@+id/user_lists_facehub"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />
    </LinearLayout>

    <FrameLayout
        android:id="@+id/magic_top_delete_constantine"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:background="@android:color/transparent"
        android:onClick="doDeleteList"
        android:visibility="gone"
        >
    </FrameLayout>
</FrameLayout>